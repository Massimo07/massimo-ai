from telegram.ext import filters, ContextTypes
from telegram.ext import ContextTypes, ContextTypes
async async def corso_avatar_ai_livello_4_handler(update, context):

    user = update.effective_user

    text = (

        "🧑‍💻 *Avatar & Voice AI – Livello 4*\n\n"

        "Crea avatar animati per presentazioni e webinar. Integra la tua voce sintetica e scegli lo stile della comunicazione."

    )

    await context.bot.send_message(chat_id=user.id, text=text)
