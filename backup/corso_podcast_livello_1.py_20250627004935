from telegram.ext import filters, ContextTypes
from telegram.ext import ContextTypes, ContextTypes
async async def corso_podcast_livello_1_handler(update, context):

    user = update.effective_user

    text = (

        "🎙️ *Podcast Pro – Livello 1*\n\n"

        "Cos’è un podcast? Registrazione audio semplice con il telefono, pubblicazione su Spotify gratis."

    )

    await context.bot.send_message(chat_id=user.id, text=text)
